{
    "name": "framework7-react-single-view",
    "private": true,
    "version": "1.3.0",
    "description": "framework7-react-single-view",
    "repository": "",
    "license": "UNLICENSED",
    "scripts": {
        "start": "npm run dev",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js",
        "watch": "webpack-cli --watch --mode production --config ./build/webpack.config.js",
        "build": "npm run build-prod",
        "build-dev": "cross-env NODE_ENV=development node ./build/build.js",
        "build-prod": "cross-env NODE_ENV=production node ./build/build.js",
        "copy-font": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/",
        "monaca:preview": "npm run dev & npm run watch",
        "monaca:transpile": "npm run build",
        "monaca:debug": "npm run watch"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-whitelist": {},
            "cordova-plugin-statusbar": {},
            "monaca-plugin-monaca-core": {},
            "cordova-plugin-geolocation": {}
        }
    },
    "browserslist": [
        "Android >= 7",
        "IOS >= 11",
        "Safari >= 11",
        "Chrome >= 49",
        "Firefox >= 31",
        "Samsung >= 5"
    ],
    "dependencies": {
        "@stripe/react-stripe-js": "^1.7.0",
        "@stripe/stripe-js": "^1.22.0",
        "cordova-plugin-splashscreen": "6.0.0",
        "cordova-plugin-statusbar": "2.4.3",
        "cordova-plugin-whitelist": "1.3.5",
        "dom7": "^3.0.0",
        "firebase": "^9.6.3",
        "framework7": "^6.3.15",
        "framework7-icons": "^5.0.5",
        "framework7-react": "^6.3.15",
        "monaca-plugin-monaca-core": "3.3.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-stripe-checkout": "^2.6.3",
        "template7": "^1.4.2"
    },
    "devDependencies": {
        "@babel/core": "^7.11.5",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "@babel/runtime": "^7.11.2",
        "babel-loader": "^8.1.0",
        "chalk": "^4.1.0",
        "copy-webpack-plugin": "^9.0.0",
        "cordova": "^11.0.0",
        "cordova-plugin-geolocation": "^4.1.0",
        "cpy-cli": "^3.1.1",
        "cross-env": "^7.0.2",
        "css-loader": "^6.5.1",
        "file-loader": "^6.1.0",
        "html-webpack-plugin": "^5.3.1",
        "mini-css-extract-plugin": "^1.6.0",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "ora": "^5.0.0",
        "postcss-loader": "^6.2.1",
        "postcss-preset-env": "^7.2.3",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^5.1.2",
        "url-loader": "^4.1.0",
        "webpack": "^5.66.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.3"
    }
}
